[main]
debug = true
log_path = /var/log/titan/

[reporting]
enabled = true
target = http://localhost:9393/titan ; Should be either route
type = syslog|http ; Valid values are syslog or http

[watcher]
enabled = true
timeout = 3 ; Time to retry to connect to iwebz
send_every = 60 ; Time to check for new reports